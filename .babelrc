{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@api": "./src/api",
          "@components": "./src/components",
          "@layouts": "./src/layouts",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
